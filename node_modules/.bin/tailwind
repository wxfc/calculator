#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wxfc/Documents/Code/calculater/calc/node_modules/tailwindcss/lib/node_modules:/Users/wxfc/Documents/Code/calculater/calc/node_modules/tailwindcss/node_modules:/Users/wxfc/Documents/Code/calculater/calc/node_modules:/Users/wxfc/Documents/Code/calculater/node_modules:/Users/wxfc/Documents/Code/node_modules:/Users/wxfc/Documents/node_modules:/Users/wxfc/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/wxfc/Documents/Code/calculater/calc/node_modules/tailwindcss/lib/node_modules:/Users/wxfc/Documents/Code/calculater/calc/node_modules/tailwindcss/node_modules:/Users/wxfc/Documents/Code/calculater/calc/node_modules:/Users/wxfc/Documents/Code/calculater/node_modules:/Users/wxfc/Documents/Code/node_modules:/Users/wxfc/Documents/node_modules:/Users/wxfc/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
