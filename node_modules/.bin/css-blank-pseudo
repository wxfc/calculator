#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wxfc/Documents/Code/calculater/calc/node_modules/css-blank-pseudo/dist/node_modules:/Users/wxfc/Documents/Code/calculater/calc/node_modules/css-blank-pseudo/node_modules:/Users/wxfc/Documents/Code/calculater/calc/node_modules:/Users/wxfc/Documents/Code/calculater/node_modules:/Users/wxfc/Documents/Code/node_modules:/Users/wxfc/Documents/node_modules:/Users/wxfc/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/wxfc/Documents/Code/calculater/calc/node_modules/css-blank-pseudo/dist/node_modules:/Users/wxfc/Documents/Code/calculater/calc/node_modules/css-blank-pseudo/node_modules:/Users/wxfc/Documents/Code/calculater/calc/node_modules:/Users/wxfc/Documents/Code/calculater/node_modules:/Users/wxfc/Documents/Code/node_modules:/Users/wxfc/Documents/node_modules:/Users/wxfc/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../css-blank-pseudo/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../css-blank-pseudo/dist/cli.cjs" "$@"
fi
